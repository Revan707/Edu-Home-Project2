    <script src="~/assetsf/js/owl.carousel.min.js"></script>
    <script src="~/assetsf/js/jquery.mb.YTPlayer.js"></script>
    <script src="~/assetsf/js/jquery.nicescroll.min.js"></script>
    <script src="~/assetsf/js/plugins.js"></script>
    <script src="~/assetsf/js/main.js"></script>

    
    
</body>

