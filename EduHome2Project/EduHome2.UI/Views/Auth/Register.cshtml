    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Favicon -->
    <link rel="shortcut icon" type="image/x-icon" href="~/assetsf/img/favicon.png">

    <link rel="stylesheet" href="~/assetsf/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/assetsf/css/animate.min.css">
    <link rel="stylesheet" href="~/assetsf/css/meanmenu.css">
    <link rel="stylesheet" href="~/assetsf/css/magnific-popup.css">
    <link rel="stylesheet" href="~/assetsf/css/owl.carousel.min.css">
    <link rel="stylesheet" href="~/assetsf/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/assetsf/css/et-line-icon.css">
    <link rel="stylesheet" href="~/assetsf/css/reset.css">
    <link rel="stylesheet" href="~/assetsf/css/ionicons.min.css">
    <link rel="stylesheet" href="~/assetsf/css/material-design-iconic-font.min.css">
    <link rel="stylesheet" href="~/assetsf/css/style.css">
    <link rel="stylesheet" href="~/assetsf/css/responsive.css">
