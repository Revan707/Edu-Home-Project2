<body>
    <!--[if lt IE 8]>
        <p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="https://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
    <![endif]-->
    <!-- Banner Area Start -->
    <div class="banner-area-wrapper">
        <div class="banner-area text-center">
            <div class="container">
                <div class="row">
                    <div class="col-12">
                        <div class="banner-content-wrapper">
                            <div class="banner-content">
                                <h2>Register</h2>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Banner Area End -->
    <!-- Login start -->
    <div class="login-area pt-150 pb-150">
        <div class="container">
            <div class="row">
                <div class="col-md-6 col-md-offset-3 text-center">
                    <div class="login">
                        <div class="login-form-container">
                            <div class="login-text">
                                <h2>Register</h2>
                            </div>
                            <div class="login-form">
                                <form method="post">
                                    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                                    <div>
                                        <span class="text-danger" asp-validation-for="Fullname"></span>
                                        <input asp-for="Fullname" placeholder="Enter fullname">
                                    </div>
                                    <div>
                                        <span class="text-danger" asp-validation-for="Username"></span>
                                        <input asp-for="Username" placeholder="Enter username">
                                    </div>
                                    <div>
                                        <span class="text-danger" asp-validation-for="Email"></span>
                                        <input asp-for="Email" placeholder="Enter email">
                                    </div>
                                    <div>
                                        <span class="text-danger" asp-validation-for="Password"></span>
                                        <input asp-for="Password" placeholder="Enter password">
                                    </div>
                                    <div>
                                        <span class="text-danger" asp-validation-for="ConfirmPassword"></span>
                                        <input asp-for="ConfirmPassword" placeholder="Enter confirmPassword">
                                    </div>
                                    <div class="button-box">
                                        <button type="submit" class="default-btn">Register</button>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Login end -->

    <script src="~/assetsf/js/vendor/jquery-3.6.0.min.js"></script>
